<odoo>
    <record id="view_users_form_inherit_commitment_date" model="ir.ui.view">
        <field name="name">res.users.form.inherit.commitment_date</field>
        <field name="model">res.users</field>
        <field name="inherit_id" ref="base.view_users_form"/>
        <field name="arch" type="xml">
            <xpath expr="//sheet/group[2]/field[@name='groups_id']" position="after">
                <field name="groups_id" widget="many2many_tags" options="{'no_create': True}" domain="[('id', '=', ref('restricciones_entregas.group_edit_commitment_date'))]"/>
            </xpath>
        </field>
    </record>
</odoo>